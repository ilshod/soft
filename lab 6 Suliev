# soft
lab6
#include <iostream>
using namespace std;
 
int main() {
    int a, b;
    char x;
    cin>>a;
    cin>>x;
    cin>>b;
if (x=='+'){
  cout<< a << x << b << "=" << a+b;
}
else
if (x=='-'){
  cout<< a << x << b << "=" << a-b;
}
else
if (x=='*'){
  cout<< a << x << b << "=" << a*b;
}
else
if (x=='/'){
  cout<< a << x << b << "=" << a/b;
}
}
